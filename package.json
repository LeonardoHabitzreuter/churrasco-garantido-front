{
  "name": "churrasco-garantido-front",
  "version": "1.0.0",
  "description": "frontend do projeto churrasco garantido",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dashboard -- node server.js",
    "test": "jest --config jest.json",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "standard --parser babel-eslint --verbose | snazzy",
    "lint:fix": "standard --parser babel-eslint --fix",
    "build": "NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/webpack --progress --config webpack/prod.config -p",
    "build:analyzer": "cross-env ANALYZER=true yarn build",
    "storybook": "start-storybook -p 6006 -c storybook",
    "storybook:build": "cross-env NODE_ENV=production BABEL_ENV=production build-storybook -c storybook",
    "update-packages": "node scripts/update.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonardoHabitzreuter/churrasco-garantido-front.git"
  },
  "author": "Leonardo Habitzreuter <leonardo.h@hbsis.com.br> (https://github.com/LeonardoHabitzreuter)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeonardoHabitzreuter/churrasco-garantido-front/issues"
  },
  "homepage": "https://github.com/LeonardoHabitzreuter/churrasco-garantido-front#readme",
  "standard": {
    "ignore": [
      "cnpjValidator.js",
      "/dist/"
    ],
    "globals": [
      "state",
      "it",
      "afterEach",
      "beforeEach",
      "describe",
      "expect",
      "localStorage",
      "jest",
      "test"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.15.0-rc.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "react-hot-loader": "^4.0.0-beta.15",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "snazzy": "^7.0.0",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "antd": "^3.3.0",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "http-server": "0.10.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "spa-http-server": "0.9.0",
    "standard": "^10.0.3"
  }
}
